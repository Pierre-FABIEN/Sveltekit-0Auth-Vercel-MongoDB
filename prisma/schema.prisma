generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model data {
  id       String @id @map("_id")
  email    String
  name     String
  password String
}

model users {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
  image String
  name  String
  role  String
}
